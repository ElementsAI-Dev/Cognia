      "formula": "公式",
      "components": {
        "inputEmbedding": {
          "name": "输入嵌入",
          "shortName": "嵌入",
          "description": "将输入词元转换为稠密向量",
          "details": "输入序列中的每个词元都通过学习的嵌入矩阵映射为 d-model 维向量。这将离散词元转换为连续向量空间。"
        },
        "positionalEncoding": {
          "name": "位置编码",
          "shortName": "位置",
          "description": "为嵌入添加位置信息",
          "details": "由于 Transformer 本身没有序列顺序的概念，需要添加位置编码来提供词元位置信息。"
        },
        "multiHeadAttention": {
          "name": "多头注意力",
          "shortName": "注意力",
          "description": "学习词元之间的关系",
          "details": "Transformer 的核心机制。计算注意力权重以确定每个词元应该关注其他词元的程度。多个注意力头学习不同的关系模式。"
        },
        "addNorm": {
          "name": "相加与归一化",
          "shortName": "Add&Norm",
          "description": "残差连接 + 层归一化",
          "details": "将输入加回到输出（残差连接）并应用层归一化。这有助于梯度流动和训练稳定性。"
        },
        "feedForward": {
          "name": "前馈网络",
          "shortName": "FFN",
          "description": "位置级全连接层",
          "details": "独立应用于每个位置的简单两层神经网络。通常先将维度扩展4倍，然后再投影回原维度。"
        },
        "encoder": {
          "name": "编码器堆栈",
          "shortName": "编码器",
          "description": "处理输入序列（N层）",
          "details": "编码器处理输入序列并创建捕获输入含义和关系的表示。通常由N=6个相同的层组成。"
        },
        "decoder": {
          "name": "解码器堆栈",
          "shortName": "解码器",
          "description": "生成输出序列（N层）",
          "details": "解码器逐个词元生成输出序列，同时关注编码器输出和先前生成的词元。同样由N=6个相同的层组成。"
        },
        "outputLinear": {
          "name": "线性投影",
          "shortName": "线性",
          "description": "投影到词表大小",
          "details": "将解码器输出映射到词表维度的线性变换，为每个可能的词元生成 logits。"
        },
        "softmax": {
          "name": "Softmax",
          "shortName": "Softmax",
          "description": "将 logits 转换为概率",
          "details": "应用 softmax 函数将原始 logits 转换为词表上的概率分布。"
        }
      },
      "steps": {
        "inputProcessing": {
          "title": "输入处理",
          "description": "输入序列首先被转换为嵌入，然后与位置编码结合。",
          "highlight": "序列 \"Hello, world!\" 被分词并转换为向量。"
        },
        "selfAttention": {
          "title": "自注意力",
          "description": "每个词元计算对所有其他词元的注意力，以理解上下文和关系。",
          "highlight": "\"world\" 这个词关注 \"Hello\" 以理解问候的上下文。"
        },
        "residualNorm1": {
          "title": "残差与归一化",
          "description": "注意力输出被加回到输入（残差连接）并归一化。"
        },
        "feedForward": {
          "title": "前馈处理",
          "description": "每个位置独立通过前馈网络处理。",
          "highlight": "这为表示添加非线性变换。"
        },
        "residualNorm2": {
          "title": "第二次残差与归一化",
          "description": "另一个残差连接和归一化完成一个编码器层。"
        },
        "fullEncoder": {
          "title": "完整编码器",
          "description": "完整的编码器堆栈通过N个相同的层处理输入。",
          "highlight": "通常N=6层，每层包含注意力和前馈子层。"
        },
        "decoderProcessing": {
          "title": "解码器处理",
          "description": "解码器生成输出词元，同时关注编码器输出和先前的词元。",
          "highlight": "使用掩码注意力防止查看未来的词元。"
        },
        "outputGeneration": {
          "title": "输出生成",
          "description": "最终输出被投影到词表大小并转换为概率。",
          "highlight": "选择概率最高的词元作为下一个输出。"
        }
      }
    }
  },
